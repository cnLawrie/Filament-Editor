            "eslint --fix",
            "git add"
        ],
        "*.ts": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "*.{json,md}": [
            "prettier --write",
            "git add"
        ],
        "*.{less,css}": [
            "stylelint --fix",
            "git add"
        ]

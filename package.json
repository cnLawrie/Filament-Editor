    "typings": "src/core/config/typings/*",
    "scripts": {
        "clean": "rimraf ./dist/*",
        "start": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js  --color --progress --hot",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.pro.config.js",
        "local": "npm run clean && cross-env NODE_ENV=local  webpack --config build/webpack.pro.config.js",
        "internal": "npm run clean && cross-env NODE_ENV=internal  webpack --config build/webpack.pro.config.js",
        "staging": "npm run clean && cross-env NODE_ENV=staging  webpack --config build/webpack.pro.config.js",
        "lint-staged": "lint-staged",
        "xlsx": "node ./build/xlsx -s ./build/xlsx/cms -d ./src/components",
        "prettier": "prettier ./src/*/**",
        "commit": "npx git-cz"
    },
